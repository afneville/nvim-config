{
  "inline-maths": {
    "prefix": "imm",
    "body": "\\$${1}\\$${0}"
  },
  "display-maths": {
    "prefix": "dmm",
    "body": "\\$\\$${1}\\$\\$${0}"
  },
  "metadata": {
    "prefix": "meta",
    "body": [
      "---",
      "title: ${1}",
      "author: ${2}",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "---",
      "",
      "${0}"
    ]
  },
  "bold": {
    "prefix": "tbf",
    "body": "**${1}**",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "italic": {
    "prefix": "tit",
    "body": "_${1}_",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "task": {
    "prefix": "task",
    "body": "- [ ] "
  },
  "h1": {
    "prefix": "h1",
    "body": "# "
  },
  "h2": {
    "prefix": "h2",
    "body": "## "
  },
  "h3": {
    "prefix": "h3",
    "body": "### "
  },
  "h4": {
    "prefix": "h4",
    "body": "#### "
  },
  "h5": {
    "prefix": "h5",
    "body": "##### "
  },
  "h6": {
    "prefix": "h6",
    "body": "###### "
  }
}
