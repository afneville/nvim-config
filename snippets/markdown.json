{
  "inline-maths": {
    "prefix": "imm",
    "body": "\\$${1}\\$${0}",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "display-maths": {
    "prefix": "dmm",
    "body": "\\$\\$${1}\\$\\$${0}",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "metadata": {
    "prefix": "meta",
    "body": [
      "---",
      "title: ${1}",
      "author: ${2}",
      "date: ${CURRENT_YEAR}-${CURRENT_MONTH}-${CURRENT_DATE}",
      "---",
      "${0}"
    ]
  },
  "bold": {
    "prefix": "tbf",
    "body": "**${1}**",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "italic": {
    "prefix": "tit",
    "body": "_${1}_",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "task": {
    "prefix": "task",
    "body": "- [ ] "
  }
}
